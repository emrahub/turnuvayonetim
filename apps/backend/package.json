    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.17.3",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "superjson": "^2.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
